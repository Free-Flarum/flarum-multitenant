{
    "name": "cryental/flarum-l10n-ext-korean",
    "type": "flarum-extension",
    "description": "Korean language pack for Flarum core.",
    "keywords": ["locale"],
    "license": "MIT",
    "authors": [
    {
        "name": "Cryental",
        "email": "contact@cryental.dev"
    }
    ],
    "support": {
      "issues": "https://github.com/Cryental/flarum-l10n-ext-korean/issues",
      "source": "https://github.com/Cryental/flarum-l10n-ext-korean"
    },
    "require": {
        "flarum/core": "^0.1.0-beta.13"
    },
    "extra": {
        "flarum-extension": {
            "title": "한국어",
            "icon": {
                "image": "icon.svg",
                "backgroundColor": "#00247d",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        },
        "flarum-locale": {
            "code": "ko",
            "title": "한국어"
        }
    }
}
